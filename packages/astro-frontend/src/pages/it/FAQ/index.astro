---
export const prerender = false

import Layout from '../../../layouts/Layout.astro'
import { Breadcrumb, BreadcrumbItem, Col, Row, Section } from 'design-react-kit'
import FAQListSection from '../../../components/FAQ/FAQListSection'
import RatingSection from '../../../components/shared/RatingSection'
---

<Layout>
