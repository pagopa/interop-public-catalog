{
  "name": "pagopa-interop-public-catalog-updater",
  "version": "1.0.0",
  "private": true,
  "description": "PagoPA Interoperability Public Catalog Updater Job",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r \"dotenv-flow/config\" ./src/index.ts",
    "build": "tsc",
    "check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "3.922.0",
    "axios": "1.12.0",
    "dotenv-flow": "4.1.0",
    "pagopa-interop-public-models": "workspace:*",
    "pg": "8.16.3",
    "ts-pattern": "5.2.0",
    "zod": "3.23.8",
    "zod-validation-error": "3.3.0"
  },
  "devDependencies": {
    "@types/node": "20.19.17",
    "@types/pg": "8.15.5",
    "mkdirp": "3.0.1",
    "pagopa-interop-public-eslint-config": "workspace:*",
    "pagopa-interop-public-typescript-config": "workspace:*",
    "prettier": "3.6.2",
    "tsc-esm-fix": "2.20.27",
    "tsx": "4.19.1",
    "typescript": "5.9.3"
  }
}
